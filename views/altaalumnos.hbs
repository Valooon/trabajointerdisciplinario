  <form method="post" action="/alumnos/alta">
      <div class="form-group">
        <label for="apyn_alumno">Nombre y Apellido: <input type="text" class="form-control" id="apyn_alumno" name="apyn_alumno"> </label>
      </div>
      <div class="form-group">
        <label for="dom_alumno">Domicilio: <input type="text" class="form-control" id="dom_alumno" name="dom_alumno"> </label>
      </div>
      <div class="form-group">
        <label for="cod_postal">Ciudad
          <select class="form-control" id="cod_postal" name="cod_postal">
            <option value="7600">Mar Del Plata</option>
            <option value="7601">Batan</option>
            <option value="8000">Bahia Blanca</option>
          </select>
        </label>  
      </div>
      <div class="form-group">
        <label for="fecha_nac_alumno">Fecha de Nacimiento: <input type="text" class="form-control" id="fecha_nac_alumno" name="fecha_nac_alumno" placeholder="yyyy-mm-dd"> </label>
      </div>
      <div class="form-group">
        <label for="email_alumno">E-Mail: <input type="text" class="form-control" id="email_alumno" name="email_alumno"> </label>
      </div>
      <div class="form-group">
        <label for="grupo_sang_alumno">Grupo Sanguineo: <input type="text" class="form-control" id="grupo_sang_alumno" name="grupo_sang_alumno"> </label>
      </div>
      <div class="form-group">
        <label for="tel_fijo_alumno">Telefono Fijo: <input type="text" class="form-control" id="tel_fijo_alumno" name="tel_fijo_alumno"> </label>
      </div>
      <div class="form-group">
        <label for="tel_movil_alumno">Telefono MÃ³vil: <input type="text" class="form-control" id="tel_movil_alumno" name="tel_movil_alumno"> </label>
      </div>
      <div class="form-group">
        <label for="dni_alumno">DNI: <input type="text" class="form-control" id="dni_alumno" name="dni_alumno"> </label>
      </div>
      <button type="submit" class="btn btn-outline-dark">Agregar</button>
